{
  "name": "@example/portfolio",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "check-version": "node scripts/tools/check-version.js",
    "dev": "npm check-version && astro dev",
    "start": "npm check-version && astro dev",
    "build": "astro check && tsc --noEmit && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "npm eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --check --plugin-search-dir=. .",
    "prettier:fix": "prettier --write --plugin-search-dir=. .",
    "format": "npm run lint:fix && npm run prettier:fix && ",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "dependencies": {
    "@astrojs/image": "^0.9.1",
    "@astrojs/netlify": "^1.1.0",
    "@astrojs/react": "^1.1.4",
    "@astrojs/tailwind": "^2.0.2",
    "astro": "^1.4.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sharp": "^0.31.1",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.8",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.5.5",
    "pretty-quick": "^3.1.3",
    "typescript": "*"
  }
}
